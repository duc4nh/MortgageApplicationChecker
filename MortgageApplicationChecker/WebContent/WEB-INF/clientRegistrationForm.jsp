<%@ page language="java" contentType="text/html; charset=ISO-8859-1"
	pageEncoding="ISO-8859-1"%>
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>

<head>
<meta http-equiv="Content-Type" content="text/html; charset=ISO-8859-1">
<title>Register New Client</title>
<meta name="description" content="website description" />
<meta name="keywords" content="website keywords, website keywords" />
<meta http-equiv="content-type" content="text/html; charset=UTF-8" />
<link rel="stylesheet" type="text/css" href="../css/style.css" />
<script type="text/javascript" src="../js/modernizr-1.5.min.js"></script>
</head>

<body>
	<%@include file="header.jsp" %>
	
	<h1>Register New Client</h1>
	
	<form action="${pageContext.request.contextPath}/protected/clientRegistration" method="GET">
		<h2>First name</h2>
		<p><input type="text" name="firstname" required /></p>
		<h2>Last name</h2>
		<p><input type="text" name="lastname" required /></p>
		<h2>Phone number</h2>
		<p><input type="text" name="phoneNumber" /></p>
		<h2>Address</h2>
		<p><input type="text" name="address" /></p>
		<h2>Organisation</h2>
		<p><input type="text" name="organisation" /></p>
		<h2>DOB</h2>
		<p><input type="date" name="dob" /></p>
		<br/>
		<p><input id="form-submit-button" type="submit" value="Register" /></p>
	</form>
	
	<%@include file="footer.jsp" %>
</body>
</html>